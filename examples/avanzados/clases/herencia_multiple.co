clase Volador:
    metodo volar(self):
        imprimir "Estoy volando"
    fin
fin

clase Nadador:
    metodo nadar(self):
        imprimir "Estoy nadando"
    fin
fin

clase Pato(Volador, Nadador):
    metodo desplazar(self):
        self.volar()
        self.nadar()
    fin
fin

pato = Pato()
pato.desplazar()
